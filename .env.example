# Copy to .env and fill with real values (DO NOT commit .env)
#
# This file is used by docker-compose.yml to avoid hardcoded credentials in config.

# --- Core Postgres ---
POSTGRES_CORE_DB=healthtourism_core
POSTGRES_CORE_USER=postgres
POSTGRES_CORE_PASSWORD=change-me
POSTGRES_CORE_PORT=5432

# --- Notification MySQL ---
MYSQL_NOTIFICATION_DB=notification_db
MYSQL_NOTIFICATION_ROOT_PASSWORD=change-me
MYSQL_NOTIFICATION_PORT=3317

# --- Redis ---
REDIS_PORT=6379

# --- RabbitMQ ---
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=change-me

# --- Kafka ---
KAFKA_PORT=9092
ZOOKEEPER_PORT=2181

# --- Elasticsearch / Kibana ---
ELASTICSEARCH_PORT=9200
KIBANA_PORT=5601

# --- MinIO (Milvus dependency) ---
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=change-me
MINIO_API_PORT=9000
MINIO_CONSOLE_PORT=9001

# --- Neo4j ---
NEO4J_AUTH=neo4j/change-me

# --- Vault (DEV MODE ONLY) ---
VAULT_DEV_ROOT_TOKEN_ID=change-me
VAULT_PORT=8200

